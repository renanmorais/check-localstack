{
  "name": "check-localstack",
  "version": "1.0.0",
  "description": "Projeto para validar uso e funcionamento do localstack",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:up": "bash scripts/compose-up.sh",
    "dev:down": "bash scripts/compose-down.sh",
    "dev:clean": "docker-compose down && docker volume rm check-localstack_localstack-data || true && npm run dev:up",
    "deploy": "IS_LOCAL=true serverless deploy --stage local",
    "deploy:clean": "npm run remove && npm run dev:clean && sleep 5 && npm run deploy",
    "remove": "IS_LOCAL=true serverless remove --stage local",
    "invoke:userCreated": "IS_LOCAL=true serverless invoke -f userCreated -l",
    "invoke:httpPing": "IS_LOCAL=true serverless invoke local -f httpPing",
    "seed": "ts-node --transpile-only scripts/seed.ts",
    "test": "jest --runInBand",
    "it": "npm run dev:up && npm run deploy && npm run seed && npm test && npm run remove && npm run dev:down"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.882.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "serverless": "^3.39.0",
    "serverless-esbuild": "^1.54.1",
    "serverless-localstack": "^1.3.1",
    "serverless-offline": "^13.6.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/client-eventbridge": "^3.637.0",
    "@aws-sdk/client-sns": "^3.637.0",
    "@aws-sdk/client-sqs": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0"
  },
  "author": "",
  "license": "ISC"
}
